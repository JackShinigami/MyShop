<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:bus="clr-namespace:BUS_MyShop;assembly=BUS_MyShop"
                    xmlns:gui="clr-namespace:GUI_MyShop">

    <DataTemplate
        DataType="{x:Type bus:BUS_Products}">

        <gui:Products />
        

    </DataTemplate>

    <DataTemplate
        DataType="{x:Type bus:BUS_Categories}">

        <gui:Categories />
        
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type bus:BUS_Orders}">

        <gui:Orders />
        
    </DataTemplate>

    <DataTemplate
        DataType="{x:Type bus:BUS_Customers}">

        <gui:Customers />
        
    </DataTemplate>
    
    
</ResourceDictionary>