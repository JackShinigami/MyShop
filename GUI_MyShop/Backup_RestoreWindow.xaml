<Window x:Class="GUI_MyShop.Backup_RestoreWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI_MyShop"
        mc:Ignorable="d"
        Title="Backup_RestoreWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnBackup" Content="Backup" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="btnBackup_Click"/>
        <Button x:Name="btnRestore" Content="Restore" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="75" Click="btnRestore_Click"/>
    </Grid>
</Window>
